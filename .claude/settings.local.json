{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(grep:*)",
      "Bash(rg:*)",
      "Bash(python manage.py shell:*)",
      "Bash(find:*)",
      "Bash(source:*)",
      "Bash(python manage.py:*)",
      "Bash(python test:*)",
      "Bash(heroku logs:*)",
      "Bash(heroku ps:*)",
      "Bash(heroku config:*)",
      "Bash(npm run build:*)",
      "Bash(npm test:*)",
      "Bash(./venv/Scripts/python.exe manage.py check --database default)",
      "Bash(./venv/Scripts/pip.exe install django-filter)",
      "Bash(./venv/Scripts/pip.exe install python-dateutil)",
      "Bash(./venv/Scripts/python.exe manage.py shell:*)",
      "Bash(./venv/Scripts/python.exe manage.py showmigrations:*)",
      "Bash(./venv/Scripts/python.exe manage.py makemigrations notifications)",
      "Bash(./venv/Scripts/python.exe manage.py migrate notifications)",
      "Bash(venv/Scripts/python.exe manage.py shell:*)",
      "Bash(npm run lint)",
      "Bash(python:*)",
      "Bash(mv:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(touch:*)",
      "Bash(npm run dev:*)",
      "Bash(node:*)"
    ],
    "deny": [],
    "defaultMode": "acceptEdits"
  }
}